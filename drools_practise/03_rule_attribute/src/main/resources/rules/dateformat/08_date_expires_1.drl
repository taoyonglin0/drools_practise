package rules.dateformat;
/*
  日期格式化设置
*/
rule "date-format-01"
    date-effective "2020-05-04 11:00:00"
    when
        eval(true)
    then
        System.out.println("【date-format-01】date-effective \"2020-05-04 11:00:00\"规则执行了");
end



rule "date-format-02"
    date-effective "2020-05-04 23:59:00"
    when
        eval(true)
    then
        System.out.println("【date-format-01】date-effective \"2020-05-04 23:59:00\"规则执行了");
end