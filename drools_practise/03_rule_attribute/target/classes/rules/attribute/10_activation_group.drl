package rules.attribute;
import java.util.Calendar
import java.text.SimpleDateFormat
import java.util.Date
/*
  activation-group ""
  default:N/A
  type:String
  description: 激活分组、通过字符串定义分组名称，具有相同足名称的规则体只有一个规则被激活，
    其他规则体的LHS部分仍然为true也不会被执行。收到salience属性的影响

    如果该分组下其他的规则没有设置salience属性，则视为规则处于被激活状态，并不受到该属性的影响
*/
rule "activation-group-01"
    activation-group "activation-group-test"
    salience 10
    when
        eval(true)
    then
     System.out.println("【activation-group-01】activation-group \"activation-group-test\"\n" +
                           "    salience 10 执行了");
end


